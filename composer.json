{
    "name": "mattyrad/noun-project-php-client",
    "description": "A PHP client to interface with The Noun Project",
    "homepage": "https://github.com/MattyRad/noun-project-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Radford",
            "email": "mradford@protonmail.com",
            "homepage": "http://mradford.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "~6.0",
        "mattyrad/support": "^1.0",
        "guzzlehttp/oauth-subscriber": "^0.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpspec/prophecy": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "MattyRad\\NounProject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MattyRad\\NounProject\\Unit\\Tests\\": "tests/Unit"
        }
    }
}
